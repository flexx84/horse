<script>
$(document).ready(function() {
	var floatRight = parseInt($("#floatRight").css('top'));

	$(window).scroll(function() {
		var scrollTop = $(window).scrollTop();
        if (scrollTop > 190) {
            var newPosition = "230px";
            $("#floatRight").css('top', newPosition);
            $("#floatRight").css('position', 'fixed');
            $("#floatLeft").css('top', newPosition);
            $("#floatLeft").css('position', 'fixed');
        } else {
            var newPosition = "420px";
            $("#floatRight").css('top', newPosition);
            $("#floatRight").css('position', 'absolute');
            $("#floatLeft").css('top', newPosition);
            $("#floatLeft").css('position', 'absolute');
        }

	}).scroll();

});
</script>
<!-- ie6,7에서 사이드뷰가 게시판 목록에서 아래 사이드뷰에 가려지는 현상 수정 -->
